@model IEnumerable<Delta_Impuls.Models.location>

@{
    ViewBag.Title = "Index";
}

<h2>Locaties</h2>

<p>
    @Html.ActionLink("Locaties toevoegen", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Locaties
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.city)
        </td>
        <td>
            @Html.ActionLink("Bewerk", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Verwijder", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
