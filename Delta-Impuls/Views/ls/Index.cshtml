@model IEnumerable<Delta_Impuls.Models.ls>

@{
    ViewBag.Title = "Categorieën";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Categorie toevoegen", "Create") 
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ls1)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ls1)
        </td>
        <td>
            @Html.ActionLink("Bewerken", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Verwijderen", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
